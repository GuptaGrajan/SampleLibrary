        <item name="android:paddingBottom">@dimen/_10sdp</item>
        <item name="android:paddingLeft">@dimen/_15sdp</item>
        <item name="android:paddingTop">@dimen/_10sdp</item>
        <item name="android:paddingRight">@dimen/_15sdp</item>
        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Snackbar.Text.Action">
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/sb__btn_bg</item>
    </style>
